








































/* swiper宽高等比 */
.swiper {
	position: relative;
	width: 100%;
	padding-bottom: 66.67%;
}
.swiper__main {
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	width: 100%;
	height: 100%;
}
.swiper__cell,
.swiper__img {
	width: 100%;
	height: 100%;
}

